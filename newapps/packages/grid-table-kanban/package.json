  "license": "MIT",
  "description": "High-performance Grid-based Table and Kanban system extracted from Teable SDK",
  "homepage": "https://github.com/teableio/teable",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teableio/teable",
    "directory": "packages/grid-table-kanban"
  },
  "author": {
